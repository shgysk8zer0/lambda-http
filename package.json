{
  "name": "@shgysk8zer0/lambda-http",
  "version": "1.0.1",
  "description": "A collection of node >= 20 utilities for Netlify Functions and AWS Lambda",
  "keywords": [
    "request",
    "response",
    "fetch",
    "cors",
    "lambda",
    "netlify-functions"
  ],
  "type": "module",
  "main": "./lambda-http.cjs",
  "module": "./lambda-http.mjs",
  "unpkg": "./lambda-http.js",
  "exports": {
    ".": {
      "import": "./lambda-http.mjs",
      "require": "./lambda-http.cjs"
    },
    "./*.js": {
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*.mjs": {
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*.cjs": {
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.cjs"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "private": false,
  "scripts": {
    "test": "npm run lint:js && npm run build && npm run tests",
    "start": "netlify functions:serve",
    "preversion": "npm test",
    "prepare": "npm run build",
    "lint:js": "eslint .",
    "fix:js": "eslint . --fix",
    "build": "npm run build:js",
    "clean": "rm -f ./*.cjs",
    "build:js": "npm run clean && rollup -c rollup.config.js",
    "tests": "node ./tests/all.js",
    "create:lock": "npm i --package-lock-only --ignore-scripts --no-audit --no-fund",
    "version:bump": "npm run version:bump:patch",
    "version:bump:patch": "npm version --no-git-tag-version patch && npm run create:lock",
    "version:bump:minor": "npm version --no-git-tag-version minor && npm run create:lock",
    "version:bump:major": "npm version --no-git-tag-version major && npm run create:lock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shgysk8zer0/lambda-http.git"
  },
  "author": "Chris Zuber <admin@kernvalley.us>",
  "license": "MIT",
  "funding": [
    {
      "type": "librepay",
      "url": "https://liberapay.com/shgysk8zer0"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/shgysk8zer0"
    }
  ],
  "bugs": {
    "url": "https://github.com/shgysk8zer0/lambda-http/issues"
  },
  "homepage": "https://github.com/shgysk8zer0/lambda-http#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.7.0",
    "netlify-cli": "^17.33.4",
    "rollup": "^4.19.0",
    "@shgysk8zer0/lambda-http": "file:./"
  },
  "dependencies": {
    "@shgysk8zer0/consts": "^1.0.8"
  }
}
